      "cell_type": "code",
      "source": [
        "import cv2\n",
        "from sentence_transformers import SentenceTransformer\n",
        "import os\n",
        "\n",
        "# Load pre-trained BERT model for embeddings\n",
        "bert_model = SentenceTransformer('bert-base-uncased')\n",
        "\n",
        "def process_video(video_path):\n",
        "    cap = cv2.VideoCapture(video_path)\n",
        "    frames = []\n",
        "\n",
        "    while cap.isOpened():\n",
        "        ret, frame = cap.read()\n",
        "\n",
        "        if not ret:\n",
        "            break\n",
        "\n",
        "        # Preprocess frame as needed\n",
        "        # ...\n",
        "\n",
        "        frames.append(frame)\n",
        "\n",
        "    cap.release()\n",
        "\n",
        "    # Convert frames to BERT embeddings\n",
        "    video_embeddings = bert_model.encode(frames)\n",
        "\n",
        "    return video_embeddings\n",
        "\n",
        "# Example Usage:\n",
        "video_path = \"path/to/your/video.mp4\"\n",
        "video_embeddings = process_video(video_path)\n",
        "print(\"Video Embeddings:\", video_embeddings)\n"
      ],
